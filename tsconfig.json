{
    "compileOnSave": false,
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "declaration": true,
        "noImplicitAny": false,
        "removeComments": false,
        "moduleResolution": "node",
        "noLib": false
    },
    "formatCodeOptions": {
        "indentSize": 4,
        "tabSize": 4,
        "newLineCharacter": "\r\n",
        "convertTabsToSpaces": true,
        "insertSpaceAfterCommaDelimiter": true,
        "insertSpaceAfterSemicolonInForStatements": true,
        "insertSpaceBeforeAndAfterBinaryOperators": true,
        "insertSpaceAfterKeywordsInControlFlowStatements": true,
        "insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
        "insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": false,
        "placeOpenBraceOnNewLineForFunctions": false,
        "placeOpenBraceOnNewLineForControlBlocks": false
    },
    "filesGlob": [
        "./**/*.ts",
        "!./**/*.d.ts",
        "!./node_modules/**/*",
        "./node_modules/bobril/**/*.d.ts",
        "./node_modules/fun-model/**/*.d.ts",
        "!./examples/**/*"
    ],
    "files": [
        "./index.ts",
        "./src/component.ts",
        "./node_modules/bobril/index.d.ts",
        "./node_modules/fun-model/index.d.ts",
        "./node_modules/fun-model/src/actionFactory.d.ts",
        "./node_modules/fun-model/src/debug.d.ts",
        "./node_modules/fun-model/src/helpers.d.ts",
        "./node_modules/fun-model/src/store.d.ts"
    ],
    "exclude": [],
    "atom": {
        "rewriteTsconfig": true
    }
}
